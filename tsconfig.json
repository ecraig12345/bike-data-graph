{
  "compilerOptions": {
    "target": "ES2019",
    "lib": [
      "ES2019",
      "DOM",
      "DOM.Iterable"
    ],
    "types": [
      "jest",
      "node"
    ],
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "noUnusedLocals": true,
    "allowJs": true,
    "skipLibCheck": true,
    "noEmit": true,
    "sourceMap": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
